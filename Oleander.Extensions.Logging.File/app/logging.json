{
  "types": {
    "consoleSink": {
      "Type": "Oleander.Extensions.Logging.Console.ColoredConsoleSink, Oleander.Extensions.Logging.Console",
      "Instance": null
    },
    "fileSink": {
      "Type": "Oleander.Extensions.Logging.File.FileSink, Oleander.Extensions.Logging.File",
      "Instance": null
    }
  },

  "fileSink": {

    "Name": "fileSink",
    "OverrideExistingFile": "false",
    "MaxFileSize": 0,
    "FileNameTemplate": "{baseDirectory}/Logging/TestConfigurationChanged.log",
    "TextFormatterType": "Oleander.Extensions.Logging.File.Tests.Format1, Oleander.Extensions.Logging.File.Tests",
    "Categories": [
      "test"
    ],
    "LogLevel": "Information"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    },
    "Oleander": {
      "LogLevel": {
        "Default": "Debug",
        "test": "Information"
      }
    }
  }
}