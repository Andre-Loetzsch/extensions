{
  "types": {
    "consoleSink": {
      "Type": "Tentakel.Extensions.Logging.Console.ColoredConsoleSink, Tentakel.Extensions.Logging.Console",
      "Instance": null
    },
    "fileSink": {
      "Type": "Tentakel.Extensions.Logging.File.FileSink, Tentakel.Extensions.Logging.File",
      "Instance": null
    }
  },

  "fileSink": {

    "Name": "fileSink",
    "OverrideExistingFile": "false",
    "FileNameTemplate": "{baseDirectory}/Logging/{dateTime:yyyy}/{dateTime:MM}/{processName}/{dateTime:yyyy-MM-dd}.log",
    "TextFormatterType": "Tentakel.Extensions.Logging.File.Tests.Format2, Tentakel.Extensions.Logging.File.Tests",
    "Categories": [
      "test"
    ],
    "LogLevel": "Information"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    },
    "Tentakel": {
      "LogLevel": {
        "Default": "Debug",
        "test": "Information"
      }
    }
  }
}